<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="main_text">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="text">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="textharga">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_semibold</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="et_main">
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:shadowColor">@color/black</item>
        <item name="android:shadowDx">1.2</item>
        <item name="android:shadowDy">1.2</item>
        <item name="android:shadowRadius">1.5</item>
        <item name="android:paddingStart">8dp</item>
    </style>
    <style name="btn_main">
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/poppins_semibold</item>
    </style>
    <style name="logo">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">60dp</item>
    </style>
    <style name="textbarang">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_light</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="textregular">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="linearlayout_wrap">
        <item name="android:layout_width" >wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>